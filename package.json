{
  "name": "xiaofeng-blog",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "repository": "https://github.com/XiaofengXie16/xiaofeng-blog.git",
  "scripts": {
    "deploy": "fly deploy --remote-only",
    "build": "vite build",
    "dev": "bunx --bun vite dev",
    "lint": "oxlint --type-aware .",
    "start": "bun ./scripts/start-server.ts",
    "typecheck": "tsgo",
    "format": "oxfmt .",
    "format:check": "oxfmt --check .",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "analyze": "vite-bundle-analyzer dist/"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.2.0",
    "@tanstack/react-router": "^1.161.1",
    "@tanstack/react-start": "^1.161.1",
    "gray-matter": "^4.0.3",
    "isbot": "^5.1.35",
    "oxlint": "^1.49.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.2.0",
    "tiny-invariant": "^1.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@tanstack/react-router-devtools": "^1.161.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260219.1",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "4.0.17",
    "babel-plugin-react-compiler": "^1.0.0",
    "bun-types": "^1.3.9",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.34.0",
    "oxlint-tsgolint": "^0.11.5",
    "typescript": "^5.9.3",
    "vite-bundle-analyzer": "^1.3.6",
    "vite-tsconfig-paths": "^6.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,mjs,cjs}": "oxfmt --write",
    "*": "npm run lint"
  },
  "trustedDependencies": [
    "esbuild"
  ],
  "resolutions": {
    "esbuild": "0.27.2"
  },
  "optionalDependencies": {
    "@oxlint/linux-x64-gnu": "^1.43.0",
    "@rollup/rollup-linux-x64-gnu": "^4.57.1",
    "lightningcss-linux-x64-gnu": "^1.31.1"
  }
}
